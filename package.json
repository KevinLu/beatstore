{
  "name": "beatstore",
  "version": "1.0.0",
  "description": "a marketplace to sell your music",
  "main": "index.js",
  "engine": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "backend": "nodemon server/index.js",
    "frontend": "npm run start --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\"",
    "test": "jshint server/index.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Kevin Lu",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.844.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "file-type": "^16.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.12.2",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "multiparty": "^4.2.2",
    "saslprep": "^1.0.3",
    "sharp": "^0.27.1",
    "stripe": "^8.135.0",
    "supports-color": "^7.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "jshint": "^2.12.0",
    "nodemon": "^1.19.1"
  }
}
